{
  "name": "app",
  "private": "true",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.19.4",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-webcam": "^7.1.1",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
